{
  "name": "client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "npm-run-all": "^4.1.5",
    "chokidar-cli": "^2.1.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tailwindcss": "^1.8.8"
  },
  "scripts": {
    "watch:tailwind": "chokidar 'src/assets/css/*.css' 'src/assets/css/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "build:tailwind": "tailwind build src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind watch",
    "watch": "webpack-dev-server --progress",
    "build": "webpack --mode production"
  }
}
